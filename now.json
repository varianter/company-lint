{
  "version": 2,
  "alias": "api.lint.variantapp.com",
  "builds": [
    {
      "src": "./api/**/*.ts",
      "use": "@now/node@canary"
    }
  ],
  "env": {
    "IS_NOW": "true",
    "DB_CONNECTION_STRING": "@lint-db-connection-string",
    "DB_DATABASE_NAME": "variant-lint",
    "ACCESS_TOKEN": "@lint-access_token"
  },
  "routes": [{ "src": "/api/(.*)", "dest": "/api/$1" }]
}
